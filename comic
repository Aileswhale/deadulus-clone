          <div id="restart-button" class="comic-control-item"><img loading="lazy" src="images/RestartIcon.svg" alt=""></div>
          <div id="next-button" class="comic-control-item"><img loading="lazy" src="images/Next-Page-Text.svg" alt="" class="image-9"><img loading="lazy" src="images/Next.svg" alt=""></div>
        </div>
        <div class="w-embed w-script">
          <script>
previousButton = document.querySelectorAll('#previous-button');
restartButton = document.querySelectorAll('#restart-button');
nextButton = document.querySelectorAll('#next-button');
minusButton = document.querySelectorAll('.minus-button');
plusButton = document.querySelectorAll('.plus-button');
canvas = document.querySelectorAll('canvas');
startButton = document.querySelectorAll('#start-button');
playButton = document.querySelectorAll('#play-button');
let box = document.querySelectorAll('.first-page-wrap');
var style = document.createElement('style');
currentSize = 86;
maxSize = 120;
minSize = 50;
var Webflow = Webflow || [];
Webflow.push(function () {
  let boxHeight = $(box).height();
  let canvasHeight = $(canvas).height();
  style.innerHTML = `
    @media screen and (max-width: 768px) {
        .canvas-slider {
          height: `+canvasHeight+`px;
        }
      }
    @media screen and (min-width: 768px) {
        canvas, .canvas-slider {
          height: `+currentSize+`vh;
        }
      }
    `;
  document.head.appendChild(style);
  $(previousButton).click(function(){
		var page1 = document.querySelectorAll('.w-slider-dot:nth-child(1)');
    var page2 = document.querySelectorAll('.w-slider-dot:nth-child(2)');
    var page16 = document.querySelectorAll('.w-slider-dot:nth-child(16)');
    if (page1[0].className != 'w-slider-dot w-active') {
        $('.w-slider-arrow-left').trigger('tap');
    }
    if (page2[0].className != 'w-slider-dot w-active' && page16[0].className != 'w-slider-dot w-active' ) {
      style.innerHTML = `
        @media screen and (max-width: 768px) {
          .canvas-slider {
            height: `+canvasHeight+`px;
          }
        }
        @media screen and (min-width: 768px) {
          canvas, .canvas-slider {
            height: `+currentSize+`vh;
          }
        }
        `;
    	document.head.appendChild(style);
    }
    else {
    	style.innerHTML = `
        @media screen and (max-width: 768px) {
          .canvas-slider {
            height: `+boxHeight+`px;
          }
        }
        @media screen and (min-width: 768px) {
          canvas, .canvas-slider {
            height: `+currentSize+`vh;
          }
        }
        `;
    	document.head.appendChild(style);
    }
  });
  if(!/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ) {
    // if resized, update size
    window.addEventListener('resize', function(event) {
      style.innerHTML = `
        @media screen and (max-width: 768px) {
          .canvas-slider {
            height: `+boxHeight+`px;
          }
        }
        @media screen and (min-width: 768px) {
          canvas, .canvas-slider {
            height: `+currentSize+`vh;
          }
        }
        `;
    }, true);
  }
  $(nextButton).click(function(){
    	var page18 = document.querySelectorAll('.w-slider-dot:nth-child(18)');
      var page2 = document.querySelectorAll('.w-slider-dot:nth-child(2)');
      var page16 = document.querySelectorAll('.w-slider-dot:nth-child(16)');
      if (page18[0].className != 'w-slider-dot w-active') {
          $('.w-slider-arrow-right').trigger('tap');
      }
      if (page2[0].className != 'w-slider-dot w-active' && page16[0].className != 'w-slider-dot w-active' ) {
        style.innerHTML = `
          @media screen and (max-width: 768px) {
            .canvas-slider {
              height: `+canvasHeight+`px;
            }
          }
          @media screen and (min-width: 768px) {
            canvas, .canvas-slider {
              height: `+currentSize+`vh;
            }
          }
          `;
        document.head.appendChild(style);
      }
      else {
        style.innerHTML = `
          @media screen and (max-width: 768px) {
            .canvas-slider {
              height: `+boxHeight+`px;
            }
          }
          @media screen and (min-width: 768px) {
            canvas, .canvas-slider {
              height: `+currentSize+`vh;
            }
          }
          `;
        document.head.appendChild(style);
      }
  });
  $(restartButton).click(function(){
    var activePage = document.querySelectorAll('.w-slider-dot.w-active');
    var activeNumber = parseInt([activePage[0].innerHTML])-1;
    var activerive = `p${activeNumber}`;
    if (activerive != 'p0') {
      eval(activerive).stateMachineInputs('State Machine 1').find(i => i.name === 'Restart').fire();
    }
  });
  $(minusButton).click(function(){
    if (currentSize >= minSize){
      currentSize -= 10;
      style.innerHTML = `
        @media screen and (min-width: 768px) {
          canvas, .canvas-slider {
            height: `+currentSize+`vh;
          }
        }
      `;
      document.head.appendChild(style);
    }
  });
  $(plusButton).click(function(){
      if (currentSize <= maxSize){
        currentSize += 10;
        console.log(currentSize)
        style.innerHTML = `
          @media screen and (min-width: 768px) {
            canvas, .canvas-slider {
              height: `+currentSize+`vh;
            }
          }
        `;
        document.head.appendChild(style);
    }
  });
  $(startButton).click(function(){
    $('.w-slider-arrow-right').trigger('tap');
    style.innerHTML = `
      @media screen and (max-width: 768px) {
        .canvas-slider {
          height: `+canvasHeight+`px;
        }
      }
      @media screen and (min-width: 768px) {
        canvas, .canvas-slider {
          height: `+currentSize+`vh;
        }
      }
      `;
    document.head.appendChild(style);
  });
  $(playButton).click(function(){
    $('.play-pause-btn').trigger('click');
  });
});
</script>
        </div>
        <div class="w-embed w-script">
          <script src="https://unpkg.com/@rive-app/canvas@1.0.79"></script>
          <script>
  var Webflow = Webflow || [];
  Webflow.push(function () {
  const p1 = new rive.Rive({
      src: 'https://rococo-muffin-80a74f.netlify.app/cover.riv',
      canvas: document.getElementById('page-1'),
      autoplay: true,
      stateMachines: 'State Machine 1',
      fit: rive.Fit.cover,
      onLoad: (_) => {
          // Get the inputs via the name of the state machine
          const p1inputs = p1.stateMachineInputs('State Machine 1');
          // Find the input you want to set a value for, or trigger
          const p1Boolean = p1inputs.find(i => i.name === 'Start');
          setTimeout(
          function() 
          {
            p1.stateMachineInputs('State Machine 1').find(i => i.name === 'Start').value = true;
          }, 1000);
      },
      onStateChange: (event) => {
        if (event.data[0] === "Next Page Listener") {
             $('.w-slider-arrow-right').trigger('tap');
            let boxHeight = $(box).height();
            style.innerHTML = `
                @media screen and (max-width: 768px) {
                  .canvas-slider {
                    height: `+boxHeight+`px;
                  }
                }
                @media screen and (min-width: 768px) {
                  canvas, .canvas-slider {
                    height: `+currentSize+`vh;
                  }
                }
                `;
              document.head.appendChild(style);
        }
      }
    });
  });
</script>
          <script>
